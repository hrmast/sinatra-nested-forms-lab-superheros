
<div class="team">
    <h1><%=@team.name%></h1><br>
    <h2>Team Motto: <%=@team.motto%></h2>
</div>

<% @supers.each do |super_hero| %>
    <div class="super_hero">
            <h2>Hero Name: <%=super_hero.name %></h2><br>
            <p>Hero Power: <%=super_hero.power %></p><br>
            <p>Hero Biography: <%=super_hero.biography %></p><br>
        <% end %>
    </div>